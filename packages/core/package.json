{
  "name": "@linkbcms/core",
  "version": "0.0.14",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && rslib build",
    "build:tsdown": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && tsdown",
    "check": "biome check --write",
    "dev:tsdown": "tsdown --watch",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "machine-translate": "inlang machine translate --project project.inlang",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/linkb-app.d.ts",
      "import": "./dist/linkb-app.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "dependencies": {
    "@inlang/paraglide-js": "2.0.0-beta.26",
    "@legendapp/state": "3.0.0-beta.30",
    "@linkbcms/ui": "workspace:*",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-form": "^1.2.3",
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-table": "^8.21.2",
    "date-fns": "^4.1.0",
    "lucide-react": "0.476.0",
    "pluralize": "^8.0.0",
    "react": "^19.1.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^19.1.0",
    "react-number-format": "^5.4.4",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@inlang/cli": "^3.0.0",
    "@rsbuild/core": "^1.2.14",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rslib/core": "^0.6.2",
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/vite": "^4.0.9",
    "@types/node": "^22.13.10",
    "@types/pluralize": "^0.0.33",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rsbuild-plugin-dts": "^0.6.3",
    "tailwindcss": "^4",
    "tsdown": "^0.6.10",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
